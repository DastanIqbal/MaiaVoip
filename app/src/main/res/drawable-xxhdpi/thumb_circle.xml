<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
        <solid
            android:color="@android:color/transparent"/>
        <size
            android:height="21dp"
            android:width="21dp"/>
        <padding
            android:left="3dp"
            android:right="3dp"
            android:top="3dp"
            android:bottom="3dp"/>
        </shape>
    </item>
    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid
                android:color="@color/maia_pink"/>
            <size
                android:height="15dp"
                android:width="15dp"/>
        </shape>
    </item>
</layer-list>